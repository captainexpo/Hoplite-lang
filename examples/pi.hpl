var numerator = 4.0 
var denominator = 1.0 
var total = 0.0
var i = true 
var iterations = 100000
var j = 0
var ct = time()
while (j < iterations) {
    //print(i == true)
    if (i == true) {
        //print("ADDING")
        total += (numerator / denominator)
    } else {
        //print("SUBTRACTING")
        total -= (numerator / denominator)
    }
    denominator += 2.0
    i = !i
    j += 1
    //print(j)
}
print(total, "in", time() - ct, "seconds")
