var numerator = 4.0 
var denominator = 1.0 
var total = 0.0
var i = true 
var iterations = 1000000
var j = 0

while (j < iterations) {
    var num_dom = numerator / denominator
    if (i) {
        total = total + num_dom
    } else {
        total = total - num_dom
    }
    denominator = denominator + 2
    if (i) {
        i = false
    } else {
        i = true
    }
    j = j + 1
    print(total)
}

print(total) 
