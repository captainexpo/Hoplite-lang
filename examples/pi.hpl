var numerator = 4.0 
var denominator = 1.0 
var total = 0.0
var i = true 
var iterations = 1000000
var j = 0

while (j < iterations) {
    if (i == true) {
        //print("ADDING")
        total += (numerator / denominator)
    } else {
        //print("SUBTRACTING")
        total -= (numerator / denominator)
    }
    denominator += 2.0
    i = !i
    print(i)
    j += 1
}

print(4*total) 
